.ods-pop-in__container {
    z-index: 102;
}

.ods-pop-in__backdrop {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: transparent;
    z-index: 101;
    cursor: auto;
}

.iframe-container {
    width: 100%;
    padding: 10px;
    max-width: inherit;
    margin-left: 120px;
    margin-right: 120px;
}

.iframe-container iframe {
    border: none;
}